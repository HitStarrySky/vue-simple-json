{
  "name": "vue-simple-json",
  "version": "1.0.2",
  "description": "Minimalist JSON editor",
  "main": "lib/index.js",
  "author": "Thezero",
  "license": "MIT",
  "private": false,
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "yarn cg:version && rimraf lib &&rollup -c ./build/rollup.config.js",
    "build:style": "gulp --gulpfile build/gulp.compile.js",
    "cg:version": "node bin/version.js",
    "commit": "git-cz",
    "lint": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue,.ts \"package/**/*.{jsx,js,ts,vue}\"",
    "lint:style": "stylelint \"package/**/*.{vue,less}\" --config ./.stylelintrc --fix",
    "log": "npm run changelog",
    "changelog": "conventional-changelog -p custom-config  -i CHANGELOG.md  -s -r 0 -n ./changelog.config.js",
    "push": "git push origin master && git push githup master"
  },
  "keywords": [
    "json",
    "typescript",
    "vue3",
    "json-editor"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/lodash.clonedeep": "^4.5.6",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.2",
    "conventional-changelog-cli": "^2.0.34",
    "conventional-changelog-custom-config": "^0.3.1",
    "cz-conventional-changelog": "^3.2.0",
    "gulp": "^4.0.2",
    "husky": "^4.3.0",
    "less": "^4.1.2",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^10.3.0",
    "postcss": "^8.4.7",
    "rollup": "^2.56.3",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "style-loader": "^3.2.1",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue": "^3.2.16",
    "vue-simple-json": "^1.0.1",
    "vue-tsc": "^0.29.8"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}