{
  "name": "simple-json",
  "version": "1.0.0",
  "description": "Minimalist JSON editor",
  "main": "index.js",
  "author": "Thezero",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "commit": "git-cz",
    "lint": "eslint -c ./.eslintrc.js --fix --ext .jsx,.js,.vue,.ts \"package/**/*.{jsx,js,ts,vue}\"",
    "lint:style": "stylelint \"package/**/*.{vue,less}\" --config ./.stylelintrc --fix",
    "log": "npm run changelog",
    "changelog": "conventional-changelog -p custom-config  -i CHANGELOG.md  -s -r 0 -n ./changelog.config.js"
  },
  "keywords": [
    "json",
    "typescript",
    "vue3",
    "json-editor"
  ],
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "typescript": "^4.5.4",
    "vue": "^3.2.16",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "conventional-changelog-cli": "^2.0.34",
    "cz-conventional-changelog": "^3.2.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "style-loader": "^3.2.1",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "dependencies": {
    "less": "^4.1.1",
    "lodash.clonedeep": "^4.5.0"
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-prettier"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}